<div id="session-new">
  <div class="container">
    <h1>Login</h1>
    <%= form_for @session, url: sessions_path do |f| %>
      <p>
        <%= f.label :Email %>
        <br>
        <%= f.text_field :email %>
      </p>
      <p>
        <%= f.label :Password %>
        <br>
        <%= f.text_field :password %>
      </p>
      <p>
        <%= f.submit "Login"  %>
      </p>
      <% end -%>
  </div>
</div>
